{
    "manifest_version": 3,
    "name": "Lead Tracker",
    "version": "1.0",
    "description": "A Chrome extension to track and save leads.",
    "permissions": ["tabs", "storage"],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo.png",
            "48": "logo.png",
            "128": "logo.png"
        }
    },
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    }
}
